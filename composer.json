{
    "name": "silverstripe/demo.silverstripe.org",
    "description": "Demo SilverStripe Project",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/installer": "^4.7.0",
        "silverstripe/dynamodb": "^4.0",
        "silverstripe/crontask": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "process-timeout": 600,
        "platform": {
            "php": "7.3.0"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            ".env.example",
            ".htaccess",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/assets.yml",
            "app/_config/database.yml",
            "app/_config/email.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/_config/theme.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/_config/theme.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ],
        "public-files-installed": [
            ".htaccess",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
