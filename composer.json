{
    "name": "silverstripe/demo.silverstripe.org",
    "description": "Demo SilverStripe Project",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/installer": "^6",
        "silverstripe/dynamodb": "^6",
        "symfony/process": "^7.3"
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true,
            "composer/installers": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.3"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "project-files": [
            "app/_config/*",
            ".env.example",
            ".graphql-generated/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "project-files-installed": [
            ".env.example",
            ".graphql-generated/_manifest_exclude",
            ".htaccess",
            "app/.htaccess",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/_config/theme.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "web.config"
        ]
    }
}
