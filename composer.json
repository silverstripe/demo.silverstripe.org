{
    "name": "silverstripe/demo.silverstripe.org",
    "description": "Demo SilverStripe Project",
    "require": {
        "php": ">=8.1.0",
        "silverstripe/installer": "^4.13.0",
        "silverstripe/dynamodb": "^4.1",
        "silverstripe/crontask": "^2.5"
    },
    "config": {
        "process-timeout": 600,
        "platform": {
            "php": "8.1.0"
        },
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            ".env.example",
            ".graphql-generated/_manifest_exclude",
            ".htaccess",
            "app/_config/assets.yml",
            "app/_config/email.yml",
            "app/_config/theme.yml",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/assets.yml",
            "mysite/_config/database.yml",
            "mysite/_config/email.yml",
            "mysite/_config/mimevalidator.yml",
            "mysite/_config/mysite.yml",
            "mysite/_config/theme.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ],
        "public-files-installed": [
            ".htaccess",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
